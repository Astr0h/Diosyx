local mush = game:GetService("Players").LocalPlayer.Data.Coins.Value
if mush >= 36000 then
	local args = {
		[1] = "AddItemAsync",
		[2] = {
			["Amount"] = 12000,
			["Type"] = "Shoom"
		}
	}

	game:GetService("ReplicatedStorage"):WaitForChild("TradeMasterEvent"):FireServer(unpack(args))

	local args = {
		[1] = "AddItemAsync",
		[2] = {
			["Amount"] = 12000,
			["Type"] = "Shoom"
		}
	}

	game:GetService("ReplicatedStorage"):WaitForChild("TradeMasterEvent"):FireServer(unpack(args))

	local args = {
		[1] = "AddItemAsync",
		[2] = {
			["Amount"] = 12000,
			["Type"] = "Shoom"
		}
	}

	game:GetService("ReplicatedStorage"):WaitForChild("TradeMasterEvent"):FireServer(unpack(args))
end

if mush > 24000 and mush < 36000 then
	local args = {
		[1] = "AddItemAsync",
		[2] = {
			["Amount"] = 12000,
			["Type"] = "Shoom"
		}
	}

	game:GetService("ReplicatedStorage"):WaitForChild("TradeMasterEvent"):FireServer(unpack(args))

	local args = {
		[1] = "AddItemAsync",
		[2] = {
			["Amount"] = 12000,
			["Type"] = "Shoom"
		}
	}

	game:GetService("ReplicatedStorage"):WaitForChild("TradeMasterEvent"):FireServer(unpack(args))

	local args = {
		[1] = "AddItemAsync",
		[2] = {
			["Amount"] = mush,
			["Type"] = "Shoom"
		}
	}

	game:GetService("ReplicatedStorage"):WaitForChild("TradeMasterEvent"):FireServer(unpack(args))
end

if mush > 12000 and mush < 24000 then
	local args = {
		[1] = "AddItemAsync",
		[2] = {
			["Amount"] = 12000,
			["Type"] = "Shoom"
		}
	}

	local args = {
		[1] = "AddItemAsync",
		[2] = {
			["Amount"] = mush,
			["Type"] = "Shoom"
		}
	}

	game:GetService("ReplicatedStorage"):WaitForChild("TradeMasterEvent"):FireServer(unpack(args))
end

if mush <= 12000 then

	local args = {
		[1] = "AddItemAsync",
		[2] = {
			["Amount"] = mush,
			["Type"] = "Shoom"
		}
	}

	game:GetService("ReplicatedStorage"):WaitForChild("TradeMasterEvent"):FireServer(unpack(args))
end
